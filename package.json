{
	"name": "my_workspace",
	"author": "Alvaro Davi",
	"version": "0.1.0",
	"description": "Web platform (and Progressive Web App) aimed at teachers, so they can organize their tasks, projects and academic study contents.",
	"license": "GPL-3.0-only",
	"main": "/pages/_app.js",
	"private": false,
	"os": [
		"linux",
		"ubuntu",
		"win32"
	],
	"keywords": [
		"my_workspace",
		"projects",
		"tasks",
		"bibliographies"
	],
	"scripts": {
		"dev": "next dev -p 8080",
		"build": "next build",
		"start": "next start -p 3000",
		"db:setup": "yarn sequelize-cli db:drop && yarn sequelize-cli db:create && yarn sequelize-cli db:migrate && yarn sequelize-cli db:seed:all",
		"test": "jest",
		"lint": "next lint",
		"format": "prettier --write \"./**/*.{js,ts,jsx,tsx,json}\""
	},
	"dependencies": {
		"@chakra-ui/icons": "^2.0.1",
		"@chakra-ui/react": "^2.1.2",
		"axios": "^0.27.2",
		"crypto-js": "^4.1.1",
		"dotenv": "^16.0.1",
		"fs": "^0.0.1-security",
		"jsonwebtoken": "^8.5.1",
		"next": "^12.1.6",
		"next-compose-plugins": "^2.2.1",
		"next-images": "^1.8.4",
		"next-pwa": "^5.5.2",
		"nookies": "^2.5.2",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"react": "^18.1.0",
		"react-beautiful-dnd": "^13.1.0",
		"react-custom-scrollbars": "^4.2.1",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-hook-form": "^7.31.2",
		"react-icons": "^4.3.1",
		"sequelize": "^6.20.0",
		"web-vitals": "^2.1.4",
		"webpack": "^5.72.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@types/jest": "^27.5.1",
		"@types/react": "^18.0.9",
		"@types/react-custom-scrollbars": "^4.0.10",
		"@types/sequelize": "^4.28.13",
		"babel-jest": "^28.1.0",
		"eslint": "^8.16.0",
		"eslint-config-next": "^12.1.6",
		"jest": "^28.1.0",
		"prettier": "^2.6.2",
		"react-test-renderer": "^18.1.0",
		"sequelize-cli": "^6.4.1",
		"ts-jest": "^28.0.3",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.7.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlvaroDavi5/MyWorkspace"
	}
