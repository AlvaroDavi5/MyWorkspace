{
	"name": "my_workspace",
	"author": "Alvaro Davi",
	"version": "0.1.0",
	"description": "Web platform (and Progressive Web App) aimed at teachers, so they can organize their tasks, projects and academic study contents.",
	"license": "GPL-3.0-only",
	"main": "/pages/_app.js",
	"private": false,
	"os": [
		"linux",
		"ubuntu",
		"win32"
	],
	"keywords": [
		"my_workspace",
		"projects",
		"tasks",
		"bibliographies"
	],
	"scripts": {
		"dev": "next dev -p 8080",
		"build": "next build",
		"start": "next start -p 3000",
